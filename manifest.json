{
  "manifest_version": 3,
  "name": "Youtube extension test",
  "version": "1.0",
  "description": "유튜브 댓글창 조작하기",
  "icons": {
    "48": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "permissions": []
}
